{
  "name": "proyecto-fisica",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "py -m uvicorn api.api:app --reload",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "main_no_track": "py -m api.main_no_track",
    "main": "py -m api.main",
    "save_keypoints": "py -m api.save_keypoints",
    "tracker": "py -m api.tracker",
    "pip-install": "pip3 install -r requirements.txt"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "lucide-react": "^0.383.0",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
